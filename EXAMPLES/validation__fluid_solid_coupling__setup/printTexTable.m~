function [] = printTexTable(ic_deg, cases, saveR, saveT, effective_R, effective_T, err_R, err_T, stftag, ftstag)
  nsamples = 1;
  ncoef = 3;
  
  siunitxavailable = 0;
  
  for i = 1:numel(cases)
    if(cases{i}.fts0_stf1)
      if(cases{i}.ortho0_slant1)
        istfslant = i;
      else
        istfortho = i;
      end
    else
      if(cases{i}.ortho0_slant1)
        iftsslant = i;
      else
        iftsortho = i;
      end
    end
  end
  
  stfptp = '\stfptp';
  stfprp = '\stfprp';
  stfprs = '\stfprs';
  ftsptp = '\ftsptp';
  ftspts = '\ftspts';
  ftsprp = '\ftsprp';
  
  if(siunitxavailable)
    numcmd = '\num';
  else
    numcmd = ''
  
  slantedtxt = ['\num{',num2str(ic_deg),'}'];
  
  mr = '\multirow';
  
  str = '';
  str = [str, '\begin{tabular}{c|c|c|ccc}']; str=andl(str);

  str = [str, '    \hline']; str=andl(str);
  str = [str, '    ',mr,'{2}{*}{Case} & ',mr,'{2}{*}{$\theta_{\mathrm{i}}$ [\si{\degree}]} & ',mr,'{2}{*}{Coefficient} & Theoretical & Synthetic & Relative\\']; str=andl(str);
  str = [str, '    & & & Value & Value & Error [\si{\percent}]\\']; str=andl(str);
  str = [str, '    \hline\hline']; str=andl(str);

  % SOLID TO FLUID, ORTHO
  curi = istfortho;
  str = [str, '    ',mr,'{',num2str(ncoef*nsamples),'}{*}{',stftag,'} & ',mr,'{',num2str(ncoef*nsamples),'}{*}{0} & ',mr,'{',num2str(nsamples),'}{*}{',stfptp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveT{curi}),'}} & \num{',pnum(effective_T{curi}),'} & ',pnumpct(err_T{curi}),' \\']; str=andl(str);
  % str = [str, '    & & & & \num{9.9963e-7} & 0.012 \\']; str=andl(str);
  % str = [str, '    & & & & \num{9.9984e-7} & 0.009 \\']; str=andl(str);
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',stfprp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveR{curi}(1)),'}} & \num{',pnum(effective_R{curi}(1)),'} & ',pnumpct(err_R{curi}(1)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',stfprs,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveR{curi}(2)),'}} & \num{',pnum(effective_R{curi}(2)),'} & ',pnumpct(err_R{curi}(2)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT

  str = [str, '    \hline']; str=andl(str);

  % SOLID TO FLUID, SLANTED
  curi = istfslant;
  str = [str, '    ',mr,'{',num2str(ncoef*nsamples),'}{*}{',stftag,'} & ',mr,'{',num2str(ncoef*nsamples),'}{*}{',slantedtxt,'} & ',mr,'{',num2str(nsamples),'}{*}{',stfptp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveT{curi}),'}} & \num{',pnum(effective_T{curi}),'} & ',pnumpct(err_T{curi}),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',stfprp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveR{curi}(1)),'}} & \num{',pnum(effective_R{curi}(1)),'} & ',pnumpct(err_R{curi}(1)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',stfprs,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveR{curi}(2)),'}} & \num{',pnum(effective_R{curi}(2)),'} & ',pnumpct(err_R{curi}(2)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT

  str = [str, '    \hline']; str=andl(str);

  % FLUID TO SOLID, ORTHO
  curi = iftsortho;
  str = [str, '    ',mr,'{',num2str(ncoef*nsamples),'}{*}{',ftstag,'} & ',mr,'{',num2str(ncoef*nsamples),'}{*}{0} & ',mr,'{',num2str(nsamples),'}{*}{',ftsptp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveT{curi}(1)),'}} & \num{',pnum(effective_T{curi}(1)),'} & ',pnumpct(err_T{curi}(1)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',ftspts,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveT{curi}(2)),'}} & \num{',pnum(effective_T{curi}(2)),'} & ',pnumpct(err_T{curi}(2)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',ftsprp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveR{curi}),'}} & \num{',pnum(effective_R{curi}),'} & ',pnumpct(err_R{curi}),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT

  str = [str, '    \hline']; str=andl(str);

  % FLUID TO SOLID, SLANTED
  curi = iftsslant;
  str = [str, '    ',mr,'{',num2str(ncoef*nsamples),'}{*}{',ftstag,'} & ',mr,'{',num2str(ncoef*nsamples),'}{*}{',slantedtxt,'} & ',mr,'{',num2str(nsamples),'}{*}{',ftsptp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveT{curi}(1)),'}} & \num{',pnum(effective_T{curi}(1)),'} & ',pnumpct(err_T{curi}(1)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',ftspts,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveT{curi}(2)),'}} & \num{',pnum(effective_T{curi}(2)),'} & ',pnumpct(err_T{curi}(2)),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT
  str = [str, '    & & ',mr,'{',num2str(nsamples),'}{*}{',ftsprp,'} & ',mr,'{',num2str(nsamples),'}{*}{\num{',pnum(saveR{curi}),'}} & \num{',pnum(effective_R{curi}),'} & ',pnumpct(err_R{curi}),' \\']; str=andl(str);
  % EVENTUAL OTHER MEASUREMENT POINT
  % EVENTUAL OTHER MEASUREMENT POINT

  str = [str, '    \hline']; str=andl(str);


  str = [str, '\end{tabular}']; str=andl(str);

  clipboard('copy', str);
  
  disp(['[',mfilename,'] LaTeX tabular copied to clipboard. CTRL+V somewhere to see.']);
end

function str = pnum(x)
  if(log10(x)<-2)
    str = sprintf('%.4e', x);
  else
    str = sprintf('%.4f', x);
  end
end
function str = pnumpct(x)
  if(isnan(x))
    str = '--';
  else
    str = ['\num{',sprintf('%.3f', x*100), '}'];
  end
end

function str=andl(str)
  str = [str, sprintf('\n')];
end